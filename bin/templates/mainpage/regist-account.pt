<div metal:use-macro="view.index">
  <div metal:fill-slot="content">
<!-- use -->
    <div id="container">            <!-- box begin -->
            <div class="box">
              <div class="border-top">
                <div class="border-right">
                  <div class="border-bot">
                    <div class="border-left">
                      <div class="left-top-corner">
                        <div class="right-top-corner">
                          <div class="right-bot-corner">
                            <div class="left-bot-corner">
                              <div class="inner">
                                <h2>Register Account Bank form</h2>
                                <br><br>
                                <div class="form-register">
                                  <form class="regist-form" action="${url}" method="post" name="form1" OnSubmit="JavaScript:return fncSubmit();">
                                    <p class="message"> ${message} </p>
                                    Name : <input type="text" placeholder="firstname  lastname" name="name" value="${name}"/><br>
                                    Nation ID : <input type="text" placeholder="xxxxxxxxxxxxx" maxlength="13" name="nationid" value="${nationid}" onKeyUp="if(isNaN(this.value)){ alert('กรุณากรอกตัวเลข'); this.value='';}"/><br>
                                    Birthday : <input type="text" placeholder="30/01/1990" name="brithday" value="${brithday}"/><br>
                                    Address : <input type="text" placeholder="address" name="address" value="${address}"/><br>
                                    Phone Number : <input type="text" placeholder="0xxxxxxxxx" maxlength="10" name="phonenumber" value="${phonenumber}"  onKeyUp="if(isNaN(this.value)){ alert('กรุณากรอกตัวเลข'); this.value='';}"/><br>
                                    <button name="form.submitted" value="Submit">Submit</button>
                                  </form>
                                </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>            <!-- box end --> <!-- not  use -->
          </div>
        </div>

        <script type="text/javascript" src="js/accountsubmit.js"></script>

</div>
</div>
